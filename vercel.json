{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://onlive.is-a.dev, http://localhost:3000"
        },
        {
          "key": "Access-Control-Allow-Methods", 
          "value": "GET, POST"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
 }